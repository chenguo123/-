#include<stdio.h>
#include<stdlib.h>
int main()
{
	int a[123] = { 0 }, i;
	char s[1001];
	gets(s);
	for (i = 0;s[i];i++)
	{
		a[s[i]]++;
		if(a[s[i]] == 1)
		printf("%c", s[i]);//打印除相同字符外剩余的字符串
	}
	for (i = 0;a[s[i]] != 0;i++)
	{
		printf("\n");
		printf("%c ",s[i]);//打印字符的种类
	    printf("%d\n", a[s[i]]);//打印对应字符所对应的数量
	}
	system("pause");
	return 0;
}
