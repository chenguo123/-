#include  <stdio.h>
#include <stdlib.h>
int main()
{
	int i,j, n=0;
	int flag = 0;
	int a[5] = { 1,2,3,4,5 };
	int b[3] = { 3,4,5 };
		for (i = 0;i<5;i++)
		{
			flag = 0;
			for (j = 0;j < 3;j++)
			{
				if (a[i] == b[j])
				{
					flag = 1;
					break;
				}
			}
			if (flag ==0)
			{
				a[n++] = a[i];
			}
		}
		for (i = 0;i <n;i++)
		{
				printf("%d\n", a[i]);
		}
	system("pause");
}
